{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "src/html/**/*.html", "use": "@vercel/static" },
    { "src": "src/js/**/*.js", "use": "@vercel/static" },
    { "src": "src/css/**/*.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/src/html/$1" }
  ]
}
